<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#34A2DA</item>
        <item name="colorPrimaryDark">#1785C6</item>
    </style>

    <style name="normal_Labletextstyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white1</item>
    </style>

    <style name="normal_textstyle">
        <item name="android:textColor">@color/textcolor</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="screen_margin_style">
        <item name="android:layout_marginRight">@dimen/pad3</item>
        <item name="android:layout_marginTop">@dimen/pad3</item>
        <item name="android:layout_marginLeft">@dimen/pad3</item>
    </style>

</resources>
